<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="softdonating.map.FollowMapper">

    <insert id="createFollow" parameterType="Follow" statementType="PREPARED"
            keyProperty="f_ID" useGeneratedKeys="true">
        insert into Follow (f_ID,u_ID,isbn) values
        (#{f_ID},#{u_ID},#{isbn})
    </insert>

</mapper>