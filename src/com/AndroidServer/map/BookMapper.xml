<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="softdonating.map.BookMapper">

    <insert id="createBook" parameterType="Book" statementType="PREPARED">
        insert into Book (isbn,name,author,publisher,content,photo,number,follow_number) values
        (#{isbn},#{name},#{author},#{publisher},#{content},#{photo},#{number},#{follow_number})
    </insert>

</mapper>