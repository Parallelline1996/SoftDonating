<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.softdonating.map.TakeMapper">

    <insert id="createTake" parameterType="Take" statementType="PREPARED"
            keyProperty="t_ID" useGeneratedKeys="true">
        insert into Take (t_ID,u_ID,d_ID,isbn,donate_time,take_time) values
        (#{t_ID},#{u_ID},#{d_ID},#{isbn},#{donate_time},#{take_time})
    </insert>

</mapper>